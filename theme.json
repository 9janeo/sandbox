{
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "layout": {
      "contentSize": "840px",
      "wideSize": "1100px"
    },
    "color": {
      "text": true,
      "background": true,
      "link": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultPalette": false,
      "defaultGradients": false,
      "duotone": [
        {
          "colors": [
            "#000",
            "#FFF"
          ],
          "slug": "black-and-white",
          "name": "Black and White"
        }
      ],
      "palette": [
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        },
        {
          "slug": "black",
          "color": "#000000",
          "name": "Black"
        },
        {
          "name": "Blackish",
          "slug": "blackish",
          "color": "#1B1B1B"
        },
        {
          "name": "Whiteish",
          "slug": "whiteish",
          "color": "#EFEFE"
        },
        {
          "slug": "primary",
          "color": "#D1D1E4",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#E4D1D1",
          "name": "Secondary"
        },
        {
          "name": "Purple",
          "slug": "purple",
          "color": "#663399"
        }
      ]
    },
    "spacing": {
      "padding": true,
      "margin": true,
